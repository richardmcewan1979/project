   "source": [
    "import time\n",
    "from threading import Thread\n",
    "\n",
    "def myfunc(i):\n",
    "    print(\"sleeping 5 sec from thread %d\" % i)\n",
    "    time.sleep(5)\n",
    "    print(\"finished sleeping from thread %d\" % i)\n",
    "\n",
    "for i in range(10):\n",
    "    t = Thread(target=myfunc, args=(i,))\n",
    "    t.start()"
   ]
